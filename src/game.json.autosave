      "events": [
        {
          "disabled": false,
          "folded": false,
          "type": "BuiltinCommonInstructions::JsCode",
          "inlineCode": "\n// Format numbers\n\nfunction fPunto(num) {\n    return num.toLocaleString('it', { useGrouping: true });\n}\n\nvar h = runtimeScene.getGame().getVariables().get(\"highscore\").getAsNumber();\nvar s = runtimeScene.getGame().getVariables().get(\"score\").getAsNumber();\nvar p = runtimeScene.getGame().getVariables().get(\"users\").getAsNumber();\nvar r = runtimeScene.getGame().getVariables().get(\"recorda\").getAsNumber();\nruntimeScene.getGame().getVariables().get(\"score_t\").setString(fPunto(s));\nruntimeScene.getGame().getVariables().get(\"highscore_t\").setString(fPunto(h));\nruntimeScene.getGame().getVariables().get(\"partite_t\").setString(fPunto(p));\nruntimeScene.getGame().getVariables().get(\"recorda_t\").setString(fPunto(r));\n\nwindow.addEventListener(\"message\", function (event) {\n    var msg = JSON.parse(event.data);\n    if (msg.name == \"storage\") {\n        var recordScene = runtimeScene.getVariables().get(\"highscore\").getAsNumber();\n        var recordGlobal = 0;\n        try {\n            recordGlobal = JSON.parse(atob(msg.value)).record;\n            runtimeScene.getGame().getVariables().get(\"highscore\").setNumber(recordGlobal);\n        } catch (e) {\n            runtimeScene.getGame().getVariables().get(\"highscore\").setNumber(0);\n            window.parent.postMessage('{\"name\":\"clearStorage\"}', \"*\");\n            console.error(e);\n        }\n    }\n});",
